        <mo>−<!-- − --></mo>
        <msub>
          <mi>χ<!-- χ --></mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>1</mn>
          </mrow>
        </msub>
        <mo stretchy="false">(</mo>
        <msub>
          <mrow class="MJX-TeXAtom-ORD">
            <mi mathvariant="bold">x</mi>
          </mrow>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
          </mrow>
        </msub>
        <mo stretchy="false">)</mo>
        <msub>
          <mi>χ<!-- χ --></mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
          </mrow>
        </msub>
        <mo stretchy="false">(</mo>
        <msub>
          <mrow class="MJX-TeXAtom-ORD">
            <mi mathvariant="bold">x</mi>
          </mrow>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>1</mn>
          </mrow>
        </msub>
        <mo stretchy="false">)</mo>
        <mo fence="false" stretchy="false">}</mo>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle \Psi (\mathbf {x} _{1},\mathbf {x} _{2})={\frac {1}{\sqrt {2}}}\{\chi _{1}(\mathbf {x} _{1})\chi _{2}(\mathbf {x} _{2})-\chi _{1}(\mathbf {x} _{2})\chi _{2}(\mathbf {x} _{1})\}}</annotation>
  </semantics></math></span><span class="lazy-image-placeholder" style="width: 48.915ex;height: 6.176ex;vertical-align: -2.838ex;" data-src="https://wikimedia.org/api/rest_v1/media/math/render/svg/aa8edda8f6bb545e622dcb5c43b81f0820883daa" data-alt="{\displaystyle \Psi (\mathbf {x} _{1},\mathbf {x} _{2})={\frac {1}{\sqrt {2}}}\{\chi _{1}(\mathbf {x} _{1})\chi _{2}(\mathbf {x} _{2})-\chi _{1}(\mathbf {x} _{2})\chi _{2}(\mathbf {x} _{1})\}}" data-class="mwe-math-fallback-image-inline">&nbsp;</span></span></dd>
<dd><span class="mwe-math-element"><span class="mwe-math-mathml-inline mwe-math-mathml-a11y" style="display: none;"><math xmlns="http://www.w3.org/1998/Math/MathML" alttext="{\displaystyle ={\frac {1}{\sqrt {2}}}{\begin{vmatrix}\chi _{1}(\mathbf {x} _{1})&amp;\chi _{2}(\mathbf {x} _{1})\\\chi _{1}(\mathbf {x} _{2})&amp;\chi _{2}(\mathbf {x} _{2})\end{vmatrix}}}">
  <semantics>
