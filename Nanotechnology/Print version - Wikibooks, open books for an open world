            <mi>C</mi>
            <mi>t</mi>
          </mrow>
        </msub>
        <mo>=</mo>
        <msub>
          <mi>V</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>S</mi>
          </mrow>
        </msub>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle V_{DCt}=V_{S}}</annotation>
