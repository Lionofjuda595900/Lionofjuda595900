              </msub>
              <msub>
                <mi>E</mi>
                <mrow class="MJX-TeXAtom-ORD">
                  <mi>b</mi>
                </mrow>
              </msub>
            </mrow>
            <mrow>
              <mi>π<!-- π --></mi>
